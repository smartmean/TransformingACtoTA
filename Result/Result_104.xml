<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.6//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_6.dtd'>
<nta>
  <declaration>bool Done_Template_ForkNode1_Branch1;
bool Done_Template_ForkNode1_Branch2;
broadcast chan fork_ForkNode1;
clock total_time=0;</declaration>
  <template>
    <name>Template</name>
    <declaration>clock t;</declaration>
    <location id="_Final1" x="0" y="100">
      <name x="-50" y="70">ActivityFinalNode1</name>
      </location>
    <location id="_Join1" x="300" y="200">
      <name x="250" y="170">JoinNode1_Join</name>
      </location>
    <location id="_Fork1" x="600" y="300">
      <name x="550" y="270">ForkNode1_Fork</name>
      </location>
    <location id="_Initial1" x="900" y="300">
      <name x="850" y="270">InitialNode1</name>
      </location>
    <init ref="_Initial1" />
    <transition id="_Join1__Final1">
      <source ref="_Join1" />
      <target ref="_Final1" />
      <label kind="guard" x="300" y="120">Done_Template_ForkNode1_Branch1==true &amp;&amp; Done_Template_ForkNode1_Branch2==true</label>
      </transition>
    <transition id="_Fork1__Join1_bypass">
      <source ref="_Fork1" />
      <target ref="_Join1" />
      <label kind="synchronisation" x="450" y="170">fork_ForkNode1!</label>
      </transition>
    <transition id="_Fork1__Join1_bypass">
      <source ref="_Fork1" />
      <target ref="_Join1" />
      <label kind="synchronisation" x="450" y="170">fork_ForkNode1!</label>
      </transition>
    </template>
  <template>
    <name>Template_ForkNode1_Branch1</name>
    <declaration>clock t2;</declaration>
    <location id="fork_Template_ForkNode1_Branch1" x="0" y="300">
      <name x="-50" y="270">InitialNode_Template_ForkNode1_Branch1</name>
      </location>
    <location id="_Process3" x="300" y="300">
      <name x="250" y="270">Process3</name>
      </location>
    <location id="_Join1" x="600" y="400">
      <name x="550" y="370">JoinNode1_Join</name>
      </location>
    <init ref="fork_Template_ForkNode1_Branch1" />
    <transition>
      <source ref="fork_Template_ForkNode1_Branch1" />
      <target ref="_Process3" />
      <label kind="synchronisation" x="150" y="220">fork_ForkNode1?</label>
      </transition>
    <transition id="_Process3__Join1">
      <source ref="_Process3" />
      <target ref="_Join1" />
      <label kind="assignment" x="450" y="310">Done_Template_ForkNode1_Branch1 = true</label>
      </transition>
    </template>
  <template>
    <name>Template_ForkNode1_Branch2</name>
    <declaration>clock t3;</declaration>
    <location id="fork_Template_ForkNode1_Branch2" x="0" y="400">
      <name x="-50" y="370">InitialNode_Template_ForkNode1_Branch2</name>
      </location>
    <location id="_Join1" x="300" y="500">
      <name x="250" y="470">JoinNode1_Join</name>
      </location>
    <location id="_Process4" x="600" y="500">
      <name x="550" y="470">Process4</name>
      </location>
    <init ref="fork_Template_ForkNode1_Branch2" />
    <transition>
      <source ref="fork_Template_ForkNode1_Branch2" />
      <target ref="_Process4" />
      <label kind="synchronisation" x="300" y="370">fork_ForkNode1?</label>
      </transition>
    <transition id="_Process4__Join1">
      <source ref="_Process4" />
      <target ref="_Join1" />
      <label kind="assignment" x="450" y="460">Done_Template_ForkNode1_Branch2 = true</label>
      </transition>
    </template>
  <system>T1 = Template();
T2 = Template_ForkNode1_Branch1();
T3 = Template_ForkNode1_Branch2();
system T1, T2, T3;</system>
  <queries>
    <query>
      <formula>A[] not deadlock</formula>
      <comment>Check for deadlocks</comment>
      </query>
    </queries>
  </nta>
