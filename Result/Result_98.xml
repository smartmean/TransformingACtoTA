<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.6//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_6.dtd'>
<nta>
  <declaration>bool Done_Template_ForkNode1_Branch1;
bool Done_Template_ForkNode1_Branch2;
broadcast chan fork_ForkNode1;
clock total_time=0;
int DecisionNode1;</declaration>
  <template>
    <name>Template</name>
    <declaration>clock t;</declaration>
    <location id="_Initial1" x="0" y="100">
      <name x="-50" y="70">InitialNode1</name>
      </location>
    <location id="_Process1" x="300" y="100">
      <name x="250" y="70">Process1</name>
      </location>
    <location id="_Decision1" x="600" y="200">
      <name x="550" y="170">DecisionNode1_Decision</name>
      </location>
    <location id="_Process2A" x="900" y="200">
      <name x="850" y="170">Process2A</name>
      </location>
    <location id="_Merge1" x="1200" y="200">
      <name x="1150" y="170">MergeNode1</name>
      </location>
    <location id="_Fork1" x="1500" y="300">
      <name x="1450" y="270">ForkNode1_Fork</name>
      </location>
    <location id="_Join1" x="1800" y="400">
      <name x="1750" y="370">JoinNode1_Join</name>
      </location>
    <location id="_Final1" x="2100" y="400">
      <name x="2050" y="370">ActivityFinalNode1</name>
      </location>
    <init ref="_Initial1" />
    <transition id="_Initial1__Process1">
      <source ref="_Initial1" />
      <target ref="_Process1" />
      </transition>
    <transition id="_Process1__Decision1">
      <source ref="_Process1" />
      <target ref="_Decision1" />
      <label kind="select" x="450" y="50">i8: int[0,1]</label>
      <label kind="assignment" x="450" y="110">t:=0, DecisionNode1 = i8</label>
      </transition>
    <transition id="_Decision1__Process2A">
      <source ref="_Decision1" />
      <target ref="_Process2A" />
      </transition>
    <transition id="_Process2A__Merge1">
      <source ref="_Process2A" />
      <target ref="_Merge1" />
      </transition>
    <transition id="_Merge1__Fork1">
      <source ref="_Merge1" />
      <target ref="_Fork1" />
      </transition>
    <transition id="_Join1__Final1">
      <source ref="_Join1" />
      <target ref="_Final1" />
      <label kind="guard" x="1800" y="320">Done_Template_ForkNode1_Branch1==true &amp;&amp; Done_Template_ForkNode1_Branch2==true</label>
      </transition>
    <transition id="_Fork1__Join1_bypass">
      <source ref="_Fork1" />
      <target ref="_Join1" />
      <label kind="synchronisation" x="1650" y="270">fork_ForkNode1!</label>
      </transition>
    <transition id="_Fork1__Join1_bypass">
      <source ref="_Fork1" />
      <target ref="_Join1" />
      <label kind="synchronisation" x="1650" y="270">fork_ForkNode1!</label>
      </transition>
    </template>
  <template>
    <name>Template_ForkNode1_Branch1</name>
    <declaration>clock t2;</declaration>
    <location id="fork_Template_ForkNode1_Branch1" x="0" y="400">
      <name x="-50" y="370">InitialNode_Template_ForkNode1_Branch1</name>
      </location>
    <location id="_Join1" x="300" y="500">
      <name x="250" y="470">JoinNode1_Join</name>
      </location>
    <location id="_Process3" x="600" y="500">
      <name x="550" y="470">Process3</name>
      </location>
    <init ref="fork_Template_ForkNode1_Branch1" />
    <transition>
      <source ref="fork_Template_ForkNode1_Branch1" />
      <target ref="_Process3" />
      <label kind="synchronisation" x="300" y="370">fork_ForkNode1?</label>
      </transition>
    <transition id="_Process3__Join1">
      <source ref="_Process3" />
      <target ref="_Join1" />
      <label kind="assignment" x="450" y="460">Done_Template_ForkNode1_Branch1 = true</label>
      </transition>
    </template>
  <template>
    <name>Template_ForkNode1_Branch2</name>
    <declaration>clock t3;</declaration>
    <location id="fork_Template_ForkNode1_Branch2" x="0" y="500">
      <name x="-50" y="470">InitialNode_Template_ForkNode1_Branch2</name>
      </location>
    <location id="_Process4" x="300" y="500">
      <name x="250" y="470">Process4</name>
      </location>
    <location id="_Join1" x="600" y="600">
      <name x="550" y="570">JoinNode1_Join</name>
      </location>
    <init ref="fork_Template_ForkNode1_Branch2" />
    <transition>
      <source ref="fork_Template_ForkNode1_Branch2" />
      <target ref="_Process4" />
      <label kind="synchronisation" x="150" y="420">fork_ForkNode1?</label>
      </transition>
    <transition id="_Process4__Join1">
      <source ref="_Process4" />
      <target ref="_Join1" />
      <label kind="assignment" x="450" y="510">Done_Template_ForkNode1_Branch2 = true</label>
      </transition>
    </template>
  <system>T1 = Template();
T2 = Template_ForkNode1_Branch1();
T3 = Template_ForkNode1_Branch2();
system T1, T2, T3;</system>
  <queries>
    <query>
      <formula>A[] not deadlock</formula>
      <comment>Check for deadlocks</comment>
      </query>
    </queries>
  </nta>
